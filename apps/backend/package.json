{
  "type": "module",
  "name": "@fullstack-starter/backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tsc -p test/tsconfig.json && NODE_ENV=test node --experimental-test-coverage --test dist/test",
    "test:file": "tsc -p test/tsconfig.json && NODE_ENV=test node --test",
    "start": "pnpm build && node dist/server.js",
    "build": "rm -rf dist && tsc",
    "typecheck": "tsc --noEmit",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "nodemon",
    "dev:start": "node --watch dist/server.js",
    "migrate:new": "dbmate new",
    "migrate:up": "dbmate up && pnpm db:generate-types",
    "migrate:down": "dbmate down && pnpm db:generate-types",
    "migrate:status": "dbmate status",
    "db:generate-types": "kysely-codegen --out-file src/types/database.ts",
    "seed:run": "pnpm build:ts && node dist/scripts/seed.js run",
    "seed:reset": "pnpm build:ts && node dist/scripts/seed.js reset",
    "seed:reset-and-seed": "pnpm build:ts && node dist/scripts/seed.js reset-and-seed",
    "seed:dev": "NODE_ENV=development pnpm seed:reset-and-seed",
    "seed:test": "NODE_ENV=test pnpm seed:reset-and-seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/csrf": "^8.0.1",
    "@fastify/env": "^5.0.2",
    "@fastify/jwt": "^10.0.0",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/type-provider-typebox": "^5.2.0",
    "@fullstack-starter/shared-schemas": "workspace:*",
    "@sinclair/typebox": "^0.34.40",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "close-with-grace": "^2.2.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.0.0",
    "google-auth-library": "^10.3.0",
    "kysely": "^0.28.5",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "pino-pretty": "^13.1.1",
    "stripe": "^18.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@types/nodemailer": "^7.0.1",
    "@types/pg": "^8.15.5",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "dbmate": "^2.28.0",
    "dotenv": "^17.2.1",
    "esbuild": "^0.25.9",
    "fastify-tsconfig": "^3.0.0",
    "kysely-codegen": "^0.18.5",
    "nodemon": "^3.1.10",
    "ts-node": "^10.4.0",
    "typescript": "~5.8.2"
  }
}