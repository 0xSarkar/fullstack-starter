{
  "name": "fullstack-starter",
  "version": "1.0.0",
  "description": "Fullstack TypeScript Starter",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "pnpm --recursive --filter \"./packages/*\" build",
    "dev": "pnpm --parallel --filter \"./packages/*\" --filter \"./apps/*\" run dev",
    "start": "pnpm --parallel --filter \"./apps/*\" start",
    "build": "pnpm --recursive --filter \"./packages/*\" --filter \"./apps/*\" build",
    "dev:backend": "pnpm --filter @fullstack-starter/backend dev",
    "dev:frontend": "pnpm --filter @fullstack-starter/frontend dev",
    "build:backend": "pnpm --filter @fullstack-starter/backend build",
    "build:frontend": "pnpm --filter @fullstack-starter/frontend build",
    "clean": "rimraf node_modules **/node_modules **/dist",
    "migrate:new": "pnpm --filter @fullstack-starter/backend run migrate:new",
    "migrate:up": "pnpm --filter @fullstack-starter/backend run migrate:up",
    "migrate:down": "pnpm --filter @fullstack-starter/backend run migrate:down",
    "seed:new": "pnpm --filter @fullstack-starter/backend run seed:new",
    "seed:run": "pnpm --filter @fullstack-starter/backend run seed:run",
    "db:generate-types": "pnpm --filter @fullstack-starter/backend run db:generate-types",
    "lint": "pnpm exec eslint .",
    "typecheck": "pnpm --recursive --filter \"./packages/*\" --filter \"./apps/*\" run --if-present typecheck",
    "check": "pnpm run lint && pnpm run typecheck"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/node": "^22.1.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.12.0",
    "rimraf": "^6.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.1"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  }
}